<h1>Posts Index</h1>
<hr>
<div class="container">
  <table class="table">
    <thead>
      <tr>
        <th>User</th>
        <th>Title</th>
        <th>Time</th>
      </tr>
    </thead>
    <tbody>
      <% @posts.each do |p| %>
      <tr>
        <td><%=link_to p.user.email, p%></td>
        <td><%=p.title%></td>
        <td><%=local_time(p.created_at)%></td>
      </tr>
      <% end %>
    </tbody>
  </table>
  <%= paginate @posts %>
</div>
